generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model sampleinput{
  id                  Int @unique      //problem_id
  data                String
}

model sampleoutput{
  id                  Int @unique       //problem_id
  data                String
}

model input{
  id                  Int @unique       //problem_id
  data                String
}

model output{
  id                  Int @unique       //problem_id
  data                String
}

model source_code{
  id                  Int @unique      //problem_id
  data                String
}